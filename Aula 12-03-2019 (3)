package newpackage;

import java.util.Scanner;

public class NewClass {
    
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        
        String result;
        int idade = 15;
        
        result = (idade >= 18) ? "Maior de Idade" : "Menor de Idade";
        
        System.out.println(result);
        
