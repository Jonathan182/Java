package newpackage;
import javax.swing.JOptionPane;

public class NewClass {
     public static void main(String[] args){
         
         int num1=20, num2=10, soma=num1+num2, div=num1/num2, sub=num1-num2, mult=num1*num2;
         
         
         JOptionPane.showMessageDialog(null,"Soma: "+ soma + "\nSubtração: "+ sub +
                 "\nMultiplicação: "+ mult + "\nDivisão: "+ div); 
      }
}
